{
  "name": "passport-mock-strategy",
  "version": "1.1.1",
  "description": "Mock passport strategy for testing Node.js applications.",
  "keywords": [
    "passport mock",
    "auth mock"
  ],
  "author": {
    "name": "Christopher Szatmary",
    "email": "cs@christopherszatmary.com",
    "url": "https://christopherszatmary.com"
  },
  "repository": "cszatma/passport-mock-strategy",
  "bugs": {
    "url": "https://github.com/cszatma/passport-mock-strategy/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "passport": "git+ssh://git@github.com:passport-next/passport.git#v2.1.0",
    "@types/express": "^4.11.1",
    "@types/passport": "^0.4.5",
    "es6-promise": "^4.2.4"
  },
  "devDependencies": {
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-supertest-cookie-fix": "^1.0.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:lib && yarn run copy-flow",
    "build:lib": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf lib",
    "copy-flow": "cp flow-types/* lib/",
    "lint": "tslint -p ./tsconfig.json --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,test}/**/*.(js|ts)\"",
    "test": "jest",
    "test:watch": "jest --watch"
  }
}
